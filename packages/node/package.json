{
  "name": "posthog-node",
  "version": "5.8.6",
  "description": "PostHog Node.js integration",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PostHog/posthog-js.git",
    "directory": "packages/node"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "test:unit": "jest",
    "build": "rslib build",
    "dev": "rslib build -w",
    "prepublishOnly": "pnpm lint && pnpm test:unit && pnpm build",
    "package": "pnpm pack --out $PACKAGE_DEST/%s.tgz",
    "gen-specs": "pnpm exec api-extractor run --config ./api-extractor.jsonc --local",
    "parse-specs": "node scripts/generate-docs.mjs"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "PostHog",
    "email": "hey@posthog.com",
    "url": "https://posthog.com"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.mjs",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@posthog/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@posthog-tooling/tsconfig-base": "workspace:*",
    "jest": "catalog:",
    "@rslib/core": "catalog:"
  },
  "keywords": [
    "posthog",
    "stats",
    "analysis",
    "funnels"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.node.d.ts",
      "edge": {
        "import": "./dist/entrypoints/index.edge.mjs",
        "require": "./dist/entrypoints/index.edge.cjs",
        "default": "./dist/entrypoints/index.edge.mjs"
      },
      "node": {
        "import": "./dist/entrypoints/index.node.mjs",
        "require": "./dist/entrypoints/index.node.cjs",
        "default": "./dist/entrypoints/index.node.mjs"
      },
      "edge-light": {
        "import": "./dist/entrypoints/index.edge.mjs",
        "require": "./dist/entrypoints/index.edge.cjs",
        "default": "./dist/entrypoints/index.edge.mjs"
      },
      "workerd": {
        "import": "./dist/entrypoints/index.edge.mjs",
        "require": "./dist/entrypoints/index.edge.cjs",
        "default": "./dist/entrypoints/index.edge.mjs"
      },
      "import": "./dist/entrypoints/index.node.mjs",
      "require": "./dist/entrypoints/index.node.cjs"
    }
  }
}
