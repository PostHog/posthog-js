{
    "name": "@posthog/rrweb-record",
    "version": "0.0.34",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "vite build --watch",
        "build": "pnpm turbo run prepublish",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "check-types": "tsc -noEmit",
        "prepublish": "tsc -noEmit && vite build",
        "lint": "pnpm eslint src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PostHog/posthog-rrweb.git"
    },
    "license": "MIT",
    "type": "module",
    "main": "./dist/rrweb-record.cjs",
    "module": "./dist/rrweb-record.js",
    "unpkg": "./dist/rrweb-record.umd.cjs",
    "typings": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/rrweb-record.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/rrweb-record.cjs"
            }
        }
    },
    "files": [
        "dist",
        "package.json"
    ],
    "devDependencies": {
        "esbuild": "^0.25.0",
        "esbuild-plugin-umd-wrapper": "^3.0.0",
        "puppeteer": "^23.11.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "vite": "^6.2.0",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^1.4.0",
        "typescript": "catalog:"
    },
    "dependencies": {
        "@posthog/rrweb-types": "workspace:*",
        "@posthog/rrweb": "workspace:*",
        "@posthog/rrweb-utils": "workspace:*"
    },
    "browserslist": [
        "supports es6-class"
    ]
}
