{
    "name": "rrdom-nodejs",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite build --watch",
        "build": "pnpm turbo run prepublish",
        "check-types": "tsc -noEmit",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "prepublish": "tsc -noEmit && vite build",
        "lint": "pnpm eslint src/**/*.ts"
    },
    "keywords": [
        "rrweb",
        "rrdom-nodejs"
    ],
    "license": "MIT",
    "type": "module",
    "main": "./dist/rrdom-nodejs.umd.cjs",
    "module": "./dist/rrdom-nodejs.js",
    "unpkg": "./dist/rrdom-nodejs.umd.cjs",
    "typings": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/rrdom-nodejs.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/rrdom-nodejs.umd.cjs"
            }
        }
    },
    "files": [
        "dist",
        "package.json"
    ],
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/cssom": "^0.4.1",
        "@types/cssstyle": "^2.2.1",
        "@types/nwsapi": "^2.2.2",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "compare-versions": "^4.1.3",
        "esbuild": "^0.25.0",
        "esbuild-plugin-umd-wrapper": "^3.0.0",
        "eslint": "^8.15.0",
        "puppeteer": "^23.11.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "vite": "^6.2.0",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^1.4.0",
        "typescript": "catalog:"
    },
    "dependencies": {
        "cssom": "^0.5.0",
        "cssstyle": "^2.3.0",
        "nwsapi": "2.2.0",
        "@posthog/rrdom": "workspace:*",
        "@posthog/rrweb-types": "workspace:*"
    }
}
