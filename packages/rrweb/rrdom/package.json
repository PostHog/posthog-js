{
    "name": "@posthog/rrdom",
    "version": "0.0.34",
    "license": "MIT",
    "type": "module",
    "main": "./dist/rrdom.cjs",
    "module": "./dist/rrdom.js",
    "unpkg": "./dist/rrdom.umd.cjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/rrdom.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/rrdom.cjs"
            }
        }
    },
    "files": [
        "dist",
        "package.json"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PostHog/posthog-rrweb.git"
    },
    "scripts": {
        "dev": "vite build --watch",
        "build": "pnpm turbo run prepublish",
        "check-types": "tsc -noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "prepublish": "tsc -noEmit && vite build",
        "lint": "pnpm eslint src/**/*.ts"
    },
    "devDependencies": {
        "@posthog/rrweb-types": "*",
        "@types/jsdom": "^20.0.0",
        "esbuild": "^0.25.0",
        "happy-dom": "^15.11.7",
        "vitest": "^1.4.0",
        "esbuild-plugin-umd-wrapper": "^3.0.0",
        "jsdom": "^22.1.0",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.15.0",
        "puppeteer": "^23.11.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "typescript": "catalog:",
        "vite": "^6.2.0",
        "vite-plugin-dts": "^4.5.4"
    },
    "dependencies": {
        "@posthog/rrweb-snapshot": "workspace:*"
    }
}
